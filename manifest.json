{
  "manifest_version": 2,

  "name": "End Game Spoilers",
  "description": "Beware of Avengers End Game Spoiler.",
  "version": "1.00",
  "icons": {
          "96":"endgame.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "hide-spoiler.js"],
      "run_at": "document_end"
    }
  ]
}
